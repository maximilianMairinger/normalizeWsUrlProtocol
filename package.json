{
  "name": "normalize-ws-url-protocol",
  "version": "0.0.1",
  "description": "Use ws:// or wss:// depending on environment.",
  "main": "./app/dist/esm/normalizeWsUrlProtocol.mjs",
  "types": "./app/dist/esm/normalizeWsUrlProtocol.d.ts",
  "bin": "./app/dist/esm/cli/normalizeWsUrlProtocol-cli.js",
  "exports": {
    "node": {
      "import": "./app/dist/esm/normalizeWsUrlProtocol.mjs",
      "require": "./app/dist/cjs/normalizeWsUrlProtocol.js"
    },
    "default": "./app/dist/esm/normalizeWsUrlProtocol.mjs"
  },
  "scripts": {
    "build": "del-cli app/dist && rollup --config rollup.prod.config.js && renamer --find /\\.js$/i --replace .mjs app/dist/esm/**",
    "dev": "npm run devWeb",
    "devWeb": "rollup --config rollup.web.dev.config.js -w",
    "devNode": "rollup --config rollup.node.dev.config.js -w",
    "deploy": "npm run build && npm publish",
    "repl": "node ./repl/dist/normalizeWsUrlProtocol-repl.js",
    "start": "npm run repl",
    "buildTest": "rollup --config rollup.node.test.config.js",
    "test": "npm run buildTest && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maximilianMairinger/normalizeWsUrlProtocol.git"
  },
  "keywords": [
  "normalize",
  "ws",
  "websocket",
  "url",
  "protocol"
],
  "author": "maximilianMairinger",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maximilianMairinger/normalizeWsUrlProtocol/issues"
  },
  "homepage": "https://github.com/maximilianMairinger/normalizeWsUrlProtocol#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-typescript": "^5.0.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^13.13.4",
    "commander": "^6.2.1",
    "del-cli": "^5.0.0",
    "jest": "^26.6.3",
    "renamer": "^4.0.0",
    "rollup": "^2.18.1",
    "tslib": "2.0.0",
    "typescript": "^4.0.2",
    "webpack-merge": "^5.0.9"
  },
  "dependencies": {

  }
}
